generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model Driver {
  id             Int     @id @default(autoincrement())
  name           String
  license        String
  additionalInfo String?
}

model Customers {
  id   Int    @id @default(autoincrement())
  name String
}

model Routes {
  id Int @id @default(autoincrement())
}
